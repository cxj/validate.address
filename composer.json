        "ext-curl": "*",
        "ext-xml": "*",
        "ext-dom": "*"
    },

    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },

    "autoload": {
        "psr-4": {
            "Cxj\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Cxj\\": "tests/"
        }
    }
}
